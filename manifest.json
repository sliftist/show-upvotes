{
    "name": "show-upvotes",
    "version": "0.1",
    "description": "Show upvotes on StackOverflow by default (must have >= 1000 rep)",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://stackoverflow.com/*", "https://*.stackoverflow.com/*"],
            "js": ["./main.js"]
        }
    ],
    "permissions": [
        "https://stackoverflow.com/*",
        "https://*.stackoverflow.com/*"
    ]
}